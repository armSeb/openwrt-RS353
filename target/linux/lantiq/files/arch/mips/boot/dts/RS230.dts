/dts-v1/;

#include "danube.dtsi"

/ {
	compatible = "bintec,rs230", "lantiq,xway", "lantiq,danube";
	model = "Bintec RS230";

	chosen {
		bootargs = "console=ttyLTQ0,115200 init=/etc/preinit";

	};

	aliases {
                led-running = &status_green;
                led-boot = &status_green;
                led-wifi = &wlan_green;
                led-usb = &usb_green;
                led-dsl = &dsl_green;
        };


	memory@0 {
		device_type = "memory";
		reg = <0x0 0x4000000>;
	};

	sram@1F000000 {
		vmmc@107000 {
			status = "okay";
		};
	};

	fpi@10000000 {
		localbus@0 {
			nor-boot@0 {
				compatible = "lantiq,nor";
				bank-width = <2>;
				reg = <0 0x0 0x1000000>;
				#address-cells = <1>;
				#size-cells = <1>;

				partitions {
					compatible = "fixed-partitions";
					#address-cells = <1>;
					#size-cells = <1>;

					boardconfig:partition@0 {
						label = "logic_config";
						reg = <0x00000 0x20000>;
						read-only;
					};

					partition@20000 {
						label = "logic";
						reg = <0x20000 0x20000>;
						read-only;
					};

					partition@40000 {
						label = "bootmonitor";
						reg = <0x40000 0x20000>;
						read-only;
					};

					partition@61000 {
						label = "flash_config";
						reg = <0x61000 0x1f000>;
						read-only;
					};

					partition@80000 {
						label = "firmware";
						reg = <0x80000 0xF80000>;
					};
				};
			};

		};

		gpio: pinmux@e100b10 {
			pinctrl-names = "default";
			pinctrl-0 = <&state_default>;

			state_default: pinmux {
				stp {
					lantiq,groups = "stp";
					lantiq,function = "stp";
				};
				mdio {
                                        lantiq,groups = "mdio";
                                        lantiq,function = "mdio";
                                };
				ebu {
					lantiq,groups = "ebu cs1";
					lantiq,function = "ebu";

				};
				usb_rst {
					lantiq,pins = "io17";
					lantiq,pull = <2>;
					lantiq,output = <1>;
				};
				switch_rst {
					lantiq,pins = "io19";
					lantiq,pull = <2>;
					lantiq,output = <1>;
				};
			};
		};

		/* warning: passive port
		only works with active devices */
		ifxhcd@E101000 {
			status = "okay";
		};

	};

	keys {
                compatible = "gpio-keys-polled";
                #address-cells = <1>;
                #size-cells = <0>;
                poll-interval = <100>;

                reset {
                        label = "reset";
                        gpios = <&gpio 27 GPIO_ACTIVE_LOW>;
                        linux,code = <0x198>;
                };
	};

	leds {
		compatible = "gpio-leds";		
                wlan_green: wlan {
                        label = "rs230:green:wlan";
                        gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
                };

                dsl_green: dsl {
                        label = "rs230:green:dsl";
                        gpios = <&gpio 9 GPIO_ACTIVE_LOW>;
                };

                usb_green: usb {
                        label = "rs230:green:usb";
                        gpios = <&gpio 10 GPIO_ACTIVE_LOW>;
                };

                status_green: status {
                        label = "rs230:green:status";
                        gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
                };
	
	};

	usb_vbus: regulator-usb-vbus {
		compatible = "regulator-fixed";

		regulator-name = "USB_VBUS";

		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;

		gpio = <&gpio 20 GPIO_ACTIVE_LOW>;
		enable-active-low;
	};
};

&pci0 {
	status = "okay";
	gpio-reset = <&gpio 21 GPIO_ACTIVE_HIGH>;
};

&gsw {
	phy-mode = "mii";
	mtd-mac-address = <&boardconfig 0x428>;
	mdio-bus {
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0>;

		phy0: ethernet-phy@0 {
			reg = <0>;
			reset-gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
		};

		switch0@0 {
			compatible = "marvell,88e6161";
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0x10>;

			ports {
				#address-cells = <1>;
				#size-cells = <0>;

				port@0 {
					reg = <0>;
					label = "lan0";
				};

				port@1 {
					reg = <1>;
					label = "lan1";
				};

				port@2 {
					reg = <2>;
					label = "lan2";
				};

				port@3 {
					reg = <3>;
					label = "lan3";
				};

				port@4 {
					reg = <4>;
					label = "wan";
				};

				port@5 {
					reg = <5>;
					label = "cpu";
					fixed-link {
						speed = <1000>;
						full-duplex;
					};
				};
			};
		};
	};
};

&usb_phy {
	status = "okay";
};

&usb {
	status = "okay";
	vbus-supply = <&usb_vbus>;
};
