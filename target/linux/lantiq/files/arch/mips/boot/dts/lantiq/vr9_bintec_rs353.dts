#include "vr9.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/mips/lantiq_rcu_gphy.h>

/ {
	compatible = "bintec,rs353", "lantiq,xway", "lantiq,vr9";
	model = "Bintec-Elmeg RS353";

	chosen {
		bootargs = "console=ttyLTQ0,115200";
	};

	aliases {
		led-running = &status;
		led-boot = &status;
		led-failsafe = &status;
		led-upgrade = &status;
		led-wifi = &wlan;
		led-dsl = &dsl;
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	fpi@10000000 {
		localbus@0 {
			nor-boot@0 {
				compatible = "lantiq,nor";
				bank-width = <2>;
				reg = <0 0x0 0x2000000>;
				#address-cells = <1>;
				#size-cells = <1>;
				lantiq,ebu_swap_check;

				partitions {
					compatible = "fixed-partitions";
					#address-cells = <1>;
					#size-cells = <1>;

					boardconfig: partition@0 {
						label = "logic_config";
						reg = <0x0 0x20000>;
						read-only;
					};

					partition@0x20000 {
						label = "logic_addr";
						reg = <0x20000 0x20000>;
						read-only;
					};

					partition@0x40000 {
						label = "bootmonitor";
						reg = <0x40000 0x20000>;
						read-only;
					};

					partition@0x80000 {
						label = "flash_config";
						reg = <0x80000 0x40000>;
						read-only;
					};

					partition@0xc0000 {
						label = "firmware";
						reg = <0xc0000 0x1F40000>;
					};
				};
			};
		};

		ifxhcd@E101000 {
			status = "okay";
		};

		ifxhcd@E106000 {
			status = "okay";
		};

		pcie@d900000 {
			status = "okay";
			gpio-reset = <&gpio 18 GPIO_ACTIVE_HIGH>;
		};
	};

	i2c@0 {
		compatible = "i2c-gpio";
		sda-gpios = <&gpio 20 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		scl-gpios = <&gpio 19 (GPIO_ACTIVE_HIGH|GPIO_OPEN_DRAIN)>;
		i2c-gpio,delay-us = <2>;	/* ~100 kHz */

		#address-cells = <1>;
		#size-cells = <0>;
		rtc@30 {
			compatible = "s35390a";
			reg = <0x30>;
		};
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <100>;

		reset {
			label = "reset";
			gpios = <&gpio 34 GPIO_ACTIVE_LOW>;
			linux,code = <0x198>; /*RESET*/
		};
		function {
			label = "function";
			gpios = <&gpio 35 GPIO_ACTIVE_LOW>;
			linux,code = <0x100>; /*BTN_1*/
		};
	};

	leds: leds {
		compatible = "gpio-leds";

		wlan: wlan {
			label = "green:wlan";
			gpios = <&gpio 28 GPIO_ACTIVE_LOW>;
		};

		dsl: dsl {
			label = "green:dsl";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
		};

		usb: usb {
			label = "green:usb";
			gpios = <&gpio 11 GPIO_ACTIVE_LOW>;
			trigger-sources = <&ehci_port1>, <&ehci_port2>;
			linux,default-trigger = "usbport";
		};

		status: status {
			label = "green:status";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};
	};

	usb_vbus: regulator-usb-vbus {
		compatible = "regulator-fixed";

		regulator-name = "USB_VBUS";

		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		/* State reversed to avoid issue with kernel 4.14.132 */
		gpio = <&gpio 14 GPIO_ACTIVE_LOW>;
		enable-active-low;
	};
};

&eth0 {
        nvmem-cells = <&macaddr_boardconfig_428>;
        nvmem-cell-names = "mac-address";
};

&gphy0 {
        lantiq,gphy-mode = <GPHY_MODE_GE>;
        resets = <&reset0 31 30>, <&reset1 7 7>;
};

&gphy1 {
        lantiq,gphy-mode = <GPHY_MODE_GE>;
        resets = <&reset0 29 28>, <&reset1 6 6>;
};

&gpio {
	pinctrl-names = "default";
	pinctrl-0 = <&state_default>;

	state_default: pinmux {
		i2c {
			lantiq,pins = "io19", "io20"; /* port i2c */
			lantiq,open-drain;
			lantiq,pull = <2>;
		};
	};
};

&gswip {
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
};

&gswip_mdio {
	phy0: ethernet-phy@0 {
		reg = <0x0>;
		lantiq,led0h = <0x20>;
		lantiq,led0l = <0x00>;
		lantiq,led1h = <0x40>;
		lantiq,led1l = <0x00>;
		lantiq,led2h = <0x70>;
		lantiq,led2l = <0x03>;
	};
	phy1: ethernet-phy@1 {
		reg = <0x1>;
		lantiq,led0h = <0x20>;
		lantiq,led0l = <0x00>;
		lantiq,led1h = <0x40>;
		lantiq,led1l = <0x00>;
		lantiq,led2h = <0x70>;
		lantiq,led2l = <0x03>;
	};
	phy2: ethernet-phy@2 {
		reg = <0x2>;
		lantiq,led0h = <0x20>;
		lantiq,led0l = <0x00>;
		lantiq,led1h = <0x40>;
		lantiq,led1l = <0x00>;
		lantiq,led2h = <0x70>;
		lantiq,led2l = <0x03>;
	};
	phy11: ethernet-phy@11 {
		reg = <0x11>;
		lantiq,led0h = <0x20>;
		lantiq,led0l = <0x00>;
		lantiq,led1h = <0x40>;
		lantiq,led1l = <0x00>;
		lantiq,led2h = <0x70>;
		lantiq,led2l = <0x03>;
	};
	phy13: ethernet-phy@13 {
		reg = <0x13>;
		lantiq,led0h = <0x20>;
		lantiq,led0l = <0x00>;
		lantiq,led1h = <0x40>;
		lantiq,led1l = <0x00>;
		lantiq,led2h = <0x70>;
		lantiq,led2l = <0x03>;
	};
};

&gswip_ports {
	port@1 {
		reg = <1>;
		label = "lan3";
		phy-mode = "rgmii";
		phy-handle = <&phy0>;
	};
	port@0 {
		reg = <0>;
		label = "lan4";
		phy-mode = "rgmii";
		phy-handle = <&phy1>;
	};
	port@3 {
		reg = <5>;
		label = "lan5";
		phy-mode = "rgmii";
		phy-handle = <&phy2>;
	};
	port@4 {
		reg = <4>;
		label = "lan1";
		phy-mode = "internal";
		phy-handle = <&phy13>;
	};
	port@5 {
		reg = <2>;
		label = "lan2";
		phy-mode = "internal";
		phy-handle = <&phy11>;
	};
};

&pcie0 {
	pcie@0 {
		reg = <0 0 0 0 0>;
		#interrupt-cells = <1>;
		#size-cells = <2>;
		#address-cells = <3>;
		device_type = "pci";

		ath9k: wifi@168c,0033 {
			compatible = "pci168c,0033";
			reg = <0 0 0 0 0>;
			nvmem-cells = <&macaddr_boardconfig_428>;
			nvmem-cell-names = "mac-address";
			mac-address-increment = <9>;
		};
	};
};

&usb_phy0 {
	status = "okay";
};

&usb_phy1 {
	status = "okay";
};

&usb0 {
	status = "okay";
	vbus-supply = <&usb_vbus>;
};

&usb1 {
	status = "okay";
	vbus-supply = <&usb_vbus>;
};

&boardconfig {
        compatible = "nvmem-cells";
        #address-cells = <1>;
        #size-cells = <1>;

        macaddr_boardconfig_428: macaddr@428 {
                reg = <0x428 0x6>;
        };
};
